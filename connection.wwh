   -
     name : Connected
     internal_properties : CAAAAAgAAACs91BklxzGtVrzkSAlcI9q2uzpneX2uXWFg96myBJlYZm39XAe9pIB4s3Z7MhLaGu8/RG753EzvzNbWIWW/vyvFqCmRjlmI/X+nZO+jBjTYHMIUGMKlHhBzvA2I+ORY4ERHGAsiRFjO0sD1RRnC8yZnQhMmcY69K2Yqk4yhxXFO7a8
     procedure_id : 1510274629286245436
     type_code : 14
     code : |1-
      
      // Summary: Starts the connection procedure
      //
      PROCEDURE Connected(sLogin is string, sPassword is string)
      
      bOnUser is boolean = False
      
      // Find the user
      HReadSeek(ForumUser,Pseudo,sLogin,hIdentical)
      IF HFound(ForumUser) THEN
      	bOnUser = True
      ELSE
      	HReadSeek(ForumUser,EmailAddress,sLogin,hIdentical)
      	IF HFound(ForumUser) THEN
      		bOnUser = True
      	END
      END
      
      // Check the password
      IF bOnUser THEN
      	IF ForumUser.Password = EncryptPWD(sPassword) THEN
      		ForumUser.DateLastVisit = DateSys() + TimeSys()
      		HModify(ForumUser)
      		gnCurrentUserID = ForumUser.ForumUserID
      		RESULT "1"
      	END
      END
      	
      RESULT "0"
     type : 458752
   -
     name : ForgottenPassword
     procedure_id : 1510274629286310972
     type_code : 14
     code : |1+
      
      // Summary: Sends the password to the user
      //
      PROCEDURE ForgottenPassword()
      
      bOnUser is boolean = False
      
      sEmail is string = [
      	Dear %1,
      	
      	Please find below your forgotten password.
      	
      	Password: %2
      ]
      
      MySession is an EmailSMTPSession = SMTPSession()
      MyMessage is an Email
      
      IF EmailStartSession(MySession) THEN
      	// Session started
      	MyMessage.SenderAddress = LoadPref(EMAIL_SENDER)
      	MyMessage.Sender = LoadPref(EMAIL_SENDER)
      	Add(MyMessage.Recipient,ForumUser.EmailAddress)
      	MyMessage.Subject = "Forgotten password"
      	MyMessage.Message = StringBuild(sEmail,ForumUser.Pseudo,DecryptPWD(ForumUser.Password))
      	
      	IF EmailSendMessage(MySession,MyMessage) THEN
      		// Message sent
      		STC_Error..Visible  = False
      		RETURN	
      	ELSE
      		// Error while sending the message"
      		Error(ErrorInfo())
      	END
      	
      	EmailCloseSession(MySession)
      ELSE
      	// Unable to start a session with the SMTP server
      	Error(ErrorInfo())
      END
      
     type : 458752
